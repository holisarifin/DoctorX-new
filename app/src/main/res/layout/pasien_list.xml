<?xml version="1.0" encoding="utf-8"?>

    <ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tvId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ID"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvNamaLengkap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama Lengkap"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilNamaLengkap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvNamaPanggilan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama Panggilan"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvNahasilmaPanggilan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvJenisKelamin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jenis Kelamin"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilJenisKelamin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvTanggalLahir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tanggal Lahir"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilTanggalLahir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvStatusPernikahan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Status Pernikahan"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilStatusPernikahan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvTempatLahir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tempat Lahir"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilTempatLahir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvAlamat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alamat"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilAlamat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvProvinsi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Provinsi"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilProvinsi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvKabupatenKota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kabupaten/Kota"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilKabupatenKota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
            <TextView
                android:id="@+id/tvKodepos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kodepos"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:textStyle="bold"
                />
            <TextView
                android:id="@+id/tvhasilKodepos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_gravity="end"
                android:layout_marginEnd="20dp"
                />
        </LinearLayout>
    </ScrollView>
